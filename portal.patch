+  const variants = {
+    'on-shift': 'bg-[#C62828] text-white',
+    'failed': 'bg-[#D32F2F] text-white',
+    'issue': 'bg-[#D32F2F] text-white',
+    'pending': 'bg-[#FFA726] text-white',
+    'approved': 'bg-[#66BB6A] text-white',
+    'rejected': 'bg-[#D32F2F] text-white',
+    'unread': 'bg-[#C62828] text-white'
