+              setShowMenu(false);
+              onOperationsAlerts();
+            }}
+            className="w-full px-6 py-3 text-left hover:bg-[#F2F2F2] transition-colors border-b border-[#F2F2F2]"
+          >
+            Operations Alerts
+          </button>
+          <button
+            onClick={() => {
+              setShowMenu(false);
+              onComponents();
+            }}
+            className="w-full px-6 py-3 text-left hover:bg-[#F2F2F2] transition-colors"
+          >
+            Components Library
+          </button>
+        </div>
+      )}
